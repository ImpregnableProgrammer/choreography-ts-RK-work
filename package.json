    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6"
